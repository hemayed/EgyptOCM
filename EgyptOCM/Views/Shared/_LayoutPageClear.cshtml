<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8; CodePage=65001" />
    <meta name="viewport" content=" width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <script type="text/javascript" src="http://ecn.dev.virtualearth.net/mapcontrol/mapcontrol.ashx?v=7.0&mkt=ngt"></script>
    <script type="text/javascript" src="~/Scripts/mapCluster.js"></script>
    <script type="text/javascript" src="~/Scripts/UpdateLists.js"></script>
    <script type="text/javascript" src="~/Scripts/UpdateMultipartialViews.js"></script>


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <  <!-- DataTables CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/DataTables-1.10.6/media/css/jquery.dataTables.css">

    <!-- jQuery -->
    <script type="text/javascript" charset="utf8" src="~/Scripts/DataTables-1.10.6/media/js/jquery.js"></script>

    <!-- DataTables -->
    <script type="text/javascript" charset="utf8" src="~/Scripts/DataTables-1.10.6/media/js/jquery.dataTables.js"></script>


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" style=" font-size:large ">
                    <li>@Html.ActionLink("الصفحة الرئيسية", "Index", "Home")</li>
                    <li>@Html.ActionLink("قوائم البيانات", "DBLists", "Home")</li>
                    <li>@Html.ActionLink("اتصل بنا", "Contact", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="bodyclear">
      
        @RenderBody()
    
    </div>

    <div class="footer">
        <p>&copy; @DateTime.Now.Year - الصندوق الاجتماعى للتنمية</p>
    </div>

    @RenderSection("scripts", required: false)
   
</body>
</html>
